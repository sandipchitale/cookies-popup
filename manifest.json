{
  "name": "Cookies",
  "manifest_version": 3,
  "version": "1.0",
  "description": "Uses the chrome.cookies API to show all cookies in a popup.",
  "permissions": ["cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "cookies_worker.js",
    "type": "module"
  }
}
